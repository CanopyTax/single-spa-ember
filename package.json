{
  "name": "single-spa-ember",
  "version": "0.1.6",
  "description": "Helper functions for using single-spa and ember together.",
  "main": "lib/single-spa-ember.js",
  "scripts": {
    "clean": "rimraf lib amd",
    "build-for-bower": "webpack --config webpack.amd-config.js",
    "build-for-npm": "babel src --out-dir lib --source-maps",
    "build": "yarn clean && yarn build-for-bower && yarn build-for-npm",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CanopyTax/single-spa-ember.git"
  },
  "keywords": [
    "single-spa",
    "ember"
  ],
  "author": "Joel Denning",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/CanopyTax/single-spa-ember/issues"
  },
  "homepage": "https://github.com/CanopyTax/single-spa-ember#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0"
  }
}
